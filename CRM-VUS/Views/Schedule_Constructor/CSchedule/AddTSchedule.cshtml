@model CRM_VUS.ViewModels.PScheduleVM
@using CRM_VUS.Models
@using System;
@using System.Globalization;
@{
    ViewData["Title"] = "Временное расписание САСК";
    var startDate = DateTime.Now;
    string Monday = startDate.AddDays(-5).ToShortDateString();
    string Tuesday = startDate.AddDays(-4).ToShortDateString();
    string Wednesday = startDate.AddDays(-3).ToShortDateString();
    string Thursday = startDate.AddDays(-2).ToShortDateString();
    string Friday = startDate.AddDays(-1).ToShortDateString();
    string Saturday = startDate.ToShortDateString();
    string Sunday = startDate.AddDays(1).ToShortDateString();
    if (startDate.DayOfWeek == DayOfWeek.Monday)
    {
        Monday = startDate.ToShortDateString();
        Tuesday = startDate.AddDays(1).ToShortDateString();
        Wednesday = startDate.AddDays(2).ToShortDateString();
        Thursday = startDate.AddDays(3).ToShortDateString();
        Friday = startDate.AddDays(4).ToShortDateString();
        Saturday = startDate.AddDays(5).ToShortDateString();
        Sunday = startDate.AddDays(6).ToShortDateString();
    }
    if (startDate.DayOfWeek == DayOfWeek.Tuesday)
    {
        Monday = startDate.AddDays(-1).ToShortDateString();
        Tuesday = startDate.ToShortDateString();
        Wednesday = startDate.AddDays(1).ToShortDateString();
        Thursday = startDate.AddDays(2).ToShortDateString();
        Friday = startDate.AddDays(3).ToShortDateString();
        Saturday = startDate.AddDays(4).ToShortDateString();
        Sunday = startDate.AddDays(5).ToShortDateString();
    }
    if (startDate.DayOfWeek == DayOfWeek.Wednesday)
    {
        Monday = startDate.AddDays(-2).ToShortDateString();
        Tuesday = startDate.AddDays(-1).ToShortDateString();
        Wednesday = startDate.ToShortDateString();
        Thursday = startDate.AddDays(1).ToShortDateString();
        Friday = startDate.AddDays(2).ToShortDateString();
        Saturday = startDate.AddDays(3).ToShortDateString();
        Sunday = startDate.AddDays(4).ToShortDateString();
    }
    if (startDate.DayOfWeek == DayOfWeek.Thursday)
    {
        Monday = startDate.AddDays(-3).ToShortDateString();
        Tuesday = startDate.AddDays(-2).ToShortDateString();
        Wednesday = startDate.AddDays(-1).ToShortDateString();
        Thursday = startDate.ToShortDateString();
        Friday = startDate.AddDays(1).ToShortDateString();
        Saturday = startDate.AddDays(2).ToShortDateString();
        Sunday = startDate.AddDays(3).ToShortDateString();
    }
    if (startDate.DayOfWeek == DayOfWeek.Friday)
    {
        Monday = startDate.AddDays(-4).ToShortDateString();
        Tuesday = startDate.AddDays(-3).ToShortDateString();
        Wednesday = startDate.AddDays(-2).ToShortDateString();
        Thursday = startDate.AddDays(-1).ToShortDateString();
        Friday = startDate.ToShortDateString();
        Saturday = startDate.AddDays(1).ToShortDateString();
        Sunday = startDate.AddDays(2).ToShortDateString();
    }
    if (startDate.DayOfWeek == DayOfWeek.Saturday)
    {
        Monday = startDate.AddDays(-5).ToShortDateString();
        Tuesday = startDate.AddDays(-4).ToShortDateString();
        Wednesday = startDate.AddDays(-3).ToShortDateString();
        Thursday = startDate.AddDays(-2).ToShortDateString();
        Friday = startDate.AddDays(-1).ToShortDateString();
        Saturday = startDate.ToShortDateString();
        Sunday = startDate.AddDays(1).ToShortDateString();
    }
    if (startDate.DayOfWeek == DayOfWeek.Sunday)
    {
        Monday = startDate.AddDays(-6).ToShortDateString();
        Tuesday = startDate.AddDays(-5).ToShortDateString();
        Wednesday = startDate.AddDays(-4).ToShortDateString();
        Thursday = startDate.AddDays(-3).ToShortDateString();
        Friday = startDate.AddDays(-2).ToShortDateString();
        Saturday = startDate.AddDays(-1).ToShortDateString();
        Sunday = startDate.ToShortDateString();
    }
    var curse = TempData["GroupСurseValue"];
    int? valuecurse = (int?)curse;
    var namegroup = TempData["GroupNameValue"];
    string group = (string)namegroup;

}
<main class="bg-warning">
    <form asp-controller="TSchedule" asp-action="SaveTS">
        <div class="container pt-5 d-flex justify-content-between">
            <div class="d-flex">
                <p class="text-light px-3">Курс</p>
                <select asp-for="TSchedulesModels.Curse" class="search-select form-select">
                    @foreach (var number in Model.studentGroupsModels.Select(option => option.Curse).Distinct())
                    {
                        <option>@number</option>
                    }
                </select>
            </div>
            <div class="d-flex">
                <p class="text-light  px-3">Группа</p>
                <select asp-for="TSchedulesModels.NameGroup" class="search-select form-select">
                    @foreach (var number in Model.studentGroupsModels.Select(option => option.NameGroup).Distinct())
                    {
                        <option>@number</option>
                    }
                </select>
            </div>
            <div class="d-flex">
                <p class="text-light  px-3">Кабинет</p>
                <select asp-for="TSchedulesModels.NameRoom" class="search-select form-select ">
                    @foreach (var number in Model.ClassRoomModels.Select(option => option.NameRoom).Distinct())
                    {
                        <option>@number</option>
                    }
                </select>
            </div>
            <div class="d-flex">
                <p class="text-light  px-3">Занятие</p>
                <select asp-for="TSchedulesModels.Lessons" class="search-select form-select ">
                    @foreach (var number in Model.ClassLessonsModels.Select(option => option.Lessons).Distinct())
                    {
                        <option>@number</option>

                    }
                    <option>Выходной</option>
                </select>
            </div>
        </div>
        <div class="container d-flex">
            <div class="d-flex col-3">
                <p class="text-light px-3">Преподаватель</p>
                <select asp-for="TSchedulesModels.NameTeacher" class="search-select form-select">
                    @foreach (var number in Model.UserModels.Select(option => option.Email).Distinct())
                    {
                        <option>@number</option>
                    }
                </select>
            </div>
            <div class="d-flex col-3 px-3" >
                <p class="text-light px-3">День</p>
                <select asp-for="TSchedulesModels.Date" class="search-select form-select">
                    @foreach (var date in Model.Dates)
                    {
                        <option value="@date.ToShortDateString()">@date.ToShortDateString()</option>
                    }
                </select>
            </div>
            <div class="d-flex col-6 px-2">
                <p class="text-light px-3">Пара</p>
                <select asp-for="TSchedulesModels.NumberLessons" class="search-select form-select">
                    <option>1</option>
                    <option>2</option>
                    <option>3</option>
                    <option>4</option>
                    <option>5</option>
                    <option>6</option>
                    <option>7</option>
                </select>
                <div class="ms-5 px-5">
                    <button type="submit" class="btn btn-dark text-light">Сохранить</button>
                </div>
            </div>
        </div>
        <div class="pt-2">
        </div>
    </form>
